@model IEnumerable<UefaServiceV9.Models.TeamRanking>

@{
    ViewBag.Title = "Team Rankings";
}

<h2>@ViewBag.Title</h2>


<table class="table">
    <tr>
        <th>
            <p>Position</p>
        </th>
        <th>
            <p>Team Name</p>
        </th>
        <th>
            <p>Country Code</p>
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ranking)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RankPosition)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TeamName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CountryCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ranking)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.Id })
        </td>
    </tr>
}

</table>
